{
  "presets": [
    "@babel/typescript",
    "@babel/react",
    [
      "@babel/env",
      {
        "modules": false,
        "corejs": 3,
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-regenerator"
  ],
  "env": {
    "esm": {
      "presets": [
        "@babel/typescript",
        "@babel/react",
        [
          "@babel/env",
          {
            "modules": false
          }
        ]
      ]
    }
  }
}